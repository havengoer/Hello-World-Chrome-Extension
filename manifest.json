{
	"name": "Hello, World!",
	"version": "0.1",
	"manifest_version": 2,
	"description": "Overrides a popup window to open in new tab.",
	"browser_action": {
		"default_icon": "images/icon.png",
		"default_title": "Popup Override",
		"default_popup": "popup.html"
	},
	"content_security_policy": "default-src 'self' ",  
	"permissions": [
		"tabs"
	]
}